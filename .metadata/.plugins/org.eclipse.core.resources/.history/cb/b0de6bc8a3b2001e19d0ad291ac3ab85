package javaCode;

public class ReverseString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String S = "ABCD";
		String rev ="";
		
		int len= S.length();
		
		for(int i=len-1;i>=0;i--)
		{
			
			rev=rev+ S.charAt(i);
		}
		
		System.out.println("Reversed String is:"+ rev);

	}

}
