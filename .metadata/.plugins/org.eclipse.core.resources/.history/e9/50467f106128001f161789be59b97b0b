package CodePractise;

 class Test {
	
	void display()
	{
	   System.out.println("1");	
	}

}	
  public class Abhi extends Test
   {
	   void show()
	   {
		   System.out.println("2");	
	   }
	   
	   public static void main(String[] args)
	   {
		   Abhi obj = new Abhi();
		   obj.display();
		   obj.show();
	   }
   }

