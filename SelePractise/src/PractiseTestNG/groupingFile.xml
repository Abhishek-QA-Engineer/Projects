<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name= "Grouping Test">
	<test name= "All Group Test">
		
		<groups>
		<run>
			<include name = "Sanity"/>
			<exclude name= "Regression"/>
		</run>
		</groups>
		<classes >
			<class name= "PractiseTestNG.LoginTest"/>
			<class name ="PractiseTestNG.LogoutTest"/>
			<class name= "PractiseTestNG.PaymentInOption"/>
		</classes>
		
	</test>
</suite>